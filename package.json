{
  "name": "datelish",
  "description": "This class helps parse, format, and transform dates.",
  "main": "dist/Datelish.js",
  "scripts": {
    "prepare": "npm run dist -s",
    "dist": "npm run lint -s && npm run babel -s && npm run minify -s",
    "lint": "jshint src/",
    "babel": "babel src/ -o dist/Datelish.js",
    "minify": "uglifyjs dist/Datelish.js -mco dist/Datelish.min.js --preamble \"/*github.com/Grafluxe/datelish*/\"",
    "doc": "jsdoc -c .jsdocrc"
  },
  "author": "Leandro Silva (http://grafluxe.com)",
  "homepage": "https://github.com/Grafluxe/datelish",
  "version": "1.1.2",
  "license": "MIT",
  "repository": "Grafluxe/datelish.git",
  "keywords": [
    "Date",
    "Date utilities",
    "Date utils",
    "Date format",
    "Date formatter",
    "Date helper"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "boxy-jsdoc-template": "^1.0.0",
    "jsdoc": "^3.4.3",
    "jshint": "^2.9.4",
    "uglify-js": "^2.7.5"
  }
}
